; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CaccaDog_v1.0"
run/main_scene="res://Scenes/press.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

PlayerState="*res://src/PlayerState.gd"
Sfx="*res://Autoload/sfx.tscn"
GameParams="*res://Autoload/game_params.gd"
GameManager="*res://Autoload/game_manager.gd"

[dialogic]

directories/dch_directory={}
directories/dtl_directory={}
glossary/default_case_sensitive=true
layout/style_list=["res://Dialogic/Styles/Bubble.tres"]
layout/default_style="res://Dialogic/Styles/Bubble.tres"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
variables={
"QuestAccepted": false
}
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true

[display]

window/size/viewport_width=1024
window/size/viewport_height=1024
window/stretch/mode="viewport"
window/stretch/scale=4.0

[editor_plugins]

enabled=PackedStringArray("res://addons/virtual_joystick/plugin.cfg")

[file_customization]

folder_colors={
"res://Levels/": "yellow",
"res://Scenes/": "teal",
"res://Scripts/": "orange"
}

